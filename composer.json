{
  "name": "fusio/adapter-laravel",
  "description": "Integrates features of the laravel framework",
  "homepage": "http://fusio-project.org",
  "keywords": ["fusio-adapter"],
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "http://christoph.kappestein.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "fusio/engine": "^4.0",
    "illuminate/database": "^7.0|^8.0",
    "illuminate/events": "^7.0|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
